{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*.{html,js,css,json,ico,png,jpg,jpeg,svg,gif}",
      "use": "@vercel/static"
    },
    {
      "src": "seo-pages/**/*.{html,js,css,json,ico,png,jpg,jpeg,svg,gif}",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/quote-engine.html",
      "dest": "/frontend/quote-engine.html"
    },
    {
      "src": "/vendor-register.html",
      "dest": "/frontend/vendor-register.html"
    },
    {
      "src": "/vendor-login.html",
      "dest": "/frontend/vendor-login.html"
    },
    {
      "src": "/customer-register.html",
      "dest": "/frontend/customer-register.html"
    },
    {
      "src": "/customer-dashboard.html",
      "dest": "/frontend/customer-dashboard.html"
    },
    {
      "src": "/vendor-dashboard.html",
      "dest": "/frontend/vendor-dashboard.html"
    },
    {
      "src": "/about.html",
      "dest": "/frontend/about.html"
    },
    {
      "src": "/how-it-works.html",
      "dest": "/frontend/how-it-works.html"
    },
    {
      "src": "/find-tradespeople.html",
      "dest": "/frontend/find-tradespeople.html"
    },
    {
      "src": "/blog.html",
      "dest": "/frontend/blog.html"
    },
    {
      "src": "/help.html",
      "dest": "/frontend/help.html"
    },
    {
      "src": "/terms.html",
      "dest": "/frontend/terms.html"
    },
    {
      "src": "/privacy.html",
      "dest": "/frontend/privacy.html"
    },
    {
      "src": "/cookies.html",
      "dest": "/frontend/cookies.html"
    },
    {
      "src": "/contact.html",
      "dest": "/frontend/contact.html"
    },
    {
      "src": "/seo-pages/(.*)",
      "dest": "/seo-pages/$1"
    },
    {
      "src": "/frontend/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "cleanUrls": true
}