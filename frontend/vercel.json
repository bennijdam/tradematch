{
  "version": 2,
  "builds": [
    {
      "src": "**/*.{html,js,css,json,ico,png,jpg,jpeg,svg,gif}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/seo-pages/(.*)",
      "dest": "/seo-pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "cleanUrls": true
}