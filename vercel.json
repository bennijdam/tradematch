    { "source": "/vendor-new-leads.html", "destination": "/leads", "permanent": true },
    { "source": "/vendor-onboarding.html", "destination": "/onboarding", "permanent": true },
    { "source": "/vendor-timeline.html", "destination": "/timeline", "permanent": true },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "tradematch.uk" }],
      "destination": "https://www.tradematch.uk/$1",
      "permanent": true
    }
  ]
}
