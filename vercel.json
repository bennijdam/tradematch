{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "frontend",
  "installCommand": "echo 'No dependencies to install'",
  "framework": null,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}