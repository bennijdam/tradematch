{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "x-tradematch-debug", "value": "v2-active" },
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/apps/api/$1" },
    { "source": "/login", "destination": "/apps/web/auth-login.html" },
    { "source": "/register", "destination": "/apps/web/auth-register.html" },
    { "source": "/forgot-password", "destination": "/apps/web/auth-forgot-password.html" },
    { "source": "/dashboard", "destination": "/apps/web/vendor-dashboard.html" },
    { "source": "/profile", "destination": "/apps/web/vendor-profile.html" },
    { "source": "/messages", "destination": "/apps/web/vendor-messages.html" },
    { "source": "/settings", "destination": "/apps/web/vendor-settings.html" },
    { "source": "/leads", "destination": "/apps/web/vendor-new-leads.html" },
    { "source": "/onboarding", "destination": "/apps/web/vendor-onboarding.html" },
    { "source": "/timeline", "destination": "/apps/web/vendor-timeline.html" },
    { "source": "/404", "destination": "/apps/web/404.html" },
    { "source": "/about", "destination": "/pages/about.html" },
    { "source": "/activate", "destination": "/pages/activate.html" },
    { "source": "/analytics-new", "destination": "/pages/analytics-new.html" },
    { "source": "/api-test", "destination": "/pages/api-test.html" },
