    { "source": "/pages/vendor-settings.html", "destination": "/settings", "permanent": true },
    { "source": "/pages/vendor-new-leads.html", "destination": "/leads", "permanent": true },
    { "source": "/pages/vendor-onboarding.html", "destination": "/onboarding", "permanent": true },
    { "source": "/pages/vendor-timeline.html", "destination": "/timeline", "permanent": true },
    { "source": "/auth-login.html", "destination": "/login", "permanent": true },
    { "source": "/auth-register.html", "destination": "/register", "permanent": true },
    { "source": "/auth-forgot-password.html", "destination": "/forgot-password", "permanent": true },
    { "source": "/vendor-dashboard.html", "destination": "/dashboard", "permanent": true },
    { "source": "/vendor-profile.html", "destination": "/profile", "permanent": true },
    { "source": "/vendor-messages.html", "destination": "/messages", "permanent": true },
