    { "source": "/compare", "destination": "/pages/compare.html" },
    { "source": "/contact", "destination": "/pages/contact.html" },
    { "source": "/cookies", "destination": "/pages/cookies.html" },
    { "source": "/cost-calculator", "destination": "/pages/cost-calculator.html" },
