{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "x-tradematch-debug", "value": "v2-active" },
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/apps/api/$1" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/register", "destination": "/register.html" },
    { "source": "/signup", "destination": "/auth-signup.html" },
    { "source": "/forgot-password", "destination": "/forgot-password.html" },
    { "source": "/dashboard", "destination": "/vendor-dashboard.html" },
    { "source": "/home", "destination": "/customer-dashboard.html" },
    { "source": "/profile", "destination": "/vendor-profile.html" },
    { "source": "/messages", "destination": "/vendor-messages.html" },
    { "source": "/settings", "destination": "/vendor-settings.html" },
    { "source": "/leads", "destination": "/vendor-new-leads.html" },
    { "source": "/timeline", "destination": "/vendor-timeline.html" },
    { "source": "/vendor", "destination": "/vendor-home.html" },
    { "source": "/user", "destination": "/customer-dashboard.html" },
    { "source": "/user/dashboard", "destination": "/user-dashboard-index.html" },
    { "source": "/user/my-jobs", "destination": "/my-jobs.html" },
    { "source": "/user/quotes", "destination": "/quotes.html" },
    { "source": "/user/messages", "destination": "/user-dashboard-messages.html" },
    { "source": "/user/saved-trades", "destination": "/saved-trades.html" },
    { "source": "/user/reviews", "destination": "/user-dashboard-reviews.html" },
    { "source": "/user/notifications", "destination": "/notifications.html" },
    { "source": "/user/billing", "destination": "/billing.html" },
    { "source": "/user/billing-addons", "destination": "/billing-addons.html" },
    { "source": "/user/settings", "destination": "/user-dashboard-settings.html" },
    { "source": "/user/profile", "destination": "/user-dashboard-profile.html" },
    { "source": "/user/post-job", "destination": "/post-job.html" },
    { "source": "/user/job-details", "destination": "/job-details.html" },
    { "source": "/user/job-quotes", "destination": "/job-quotes.html" },
    { "source": "/user/your-quotes", "destination": "/your-quotes.html" },
    { "source": "/admin", "destination": "/super-admin-dashboard-index.html" },
    { "source": "/404", "destination": "/404.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/activate", "destination": "/activate.html" },
    { "source": "/analytics-new", "destination": "/analytics-new.html" },
    { "source": "/api-test", "destination": "/api-test.html" },
    { "source": "/ask-a-trade", "destination": "/ask-a-trade.html" },
    { "source": "/auth-select-role", "destination": "/auth-select-role.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/careers", "destination": "/careers.html" },
    { "source": "/compare", "destination": "/compare.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/cookies", "destination": "/cookies.html" },
    { "source": "/cost-calculator", "destination": "/cost-calculator.html" },
    { "source": "/email-preferences", "destination": "/email-preferences.html" },
