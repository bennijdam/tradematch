{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*.{html,js,css,json,ico,png,jpg,jpeg,svg,gif,woff,woff2}",
      "use": "@vercel/static"
    }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}