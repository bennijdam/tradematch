{
  "version": 2,
  "buildCommand": "echo 'No build step required - static HTML files'",
  "builds": [
    {
      "src": "frontend/**/*.{html,js,css,json,ico,png,jpg,jpeg,svg,gif,woff,woff2}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html",
      "check": true
    }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
