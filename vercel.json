    { "source": "/vendor-heatmaps", "destination": "/vendor-heatmaps.html" },
    { "source": "/vendor-impressions", "destination": "/vendor-impressions.html" },
    { "source": "/vendor-messages", "destination": "/vendor-messages.html" },
    { "source": "/vendor-profile", "destination": "/vendor-profile.html" },
    { "source": "/vendor-register", "destination": "/vendor-register.html" },
    { "source": "/vendor-service-area", "destination": "/vendor-service-area.html" },
    { "source": "/vendor-settings", "destination": "/vendor-settings.html" },
    { "source": "/(.*)", "destination": "/$1" }
  ]
}
