    { "source": "/investor-relations", "destination": "/pages/investor-relations.html" },
    { "source": "/location-service-enhanced", "destination": "/pages/location-service-enhanced.html" },
    { "source": "/messaging-system", "destination": "/pages/messaging-system.html" },
    { "source": "/milestone-manager", "destination": "/pages/milestone-manager.html" },
    { "source": "/payment-checkout", "destination": "/pages/payment-checkout.html" },
